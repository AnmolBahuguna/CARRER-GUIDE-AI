#!/usr/bin/env bash

# Example environment configuration for SmartCareer
# Copy this file to `.env` and fill in your real values. Do NOT commit `.env`.

# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_SITE_URL=http://127.0.0.1:5000
OPENROUTER_APP_TITLE=SmartCareer

# Web3Forms Configuration
WEB3FORMS_ACCESS_KEY=your_web3forms_access_key_here

# Flask Configuration
FLASK_SECRET_KEY=your_secret_key_here_change_in_production
FLASK_DEBUG=True
FLASK_ENV=development

# Session Configuration (seconds)
SESSION_TIMEOUT=3600

# CORS Configuration
CORS_ORIGINS=http://localhost:5000,http://127.0.0.1:5000
